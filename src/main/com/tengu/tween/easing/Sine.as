package com.tengu.tween.easing{    public class Sine    {        public static function easeIn (ratio:Number):Number        {            return 1 - Math.cos(ratio * (Math.PI / 2));        }        public static function easeOut (ratio:Number):Number        {            return Math.sin(ratio * (Math.PI / 2));        }        public static function easeInOut (ratio:Number):Number        {            return -0.5 * (Math.cos(ratio * Math.PI) - 1);        }    }}