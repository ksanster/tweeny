package com.tengu.tween.easing{    public class Quadratic    {        public static function easeIn (ratio:Number):Number        {            return ratio * ratio;        }        public static function easeOut (ratio:Number):Number        {            return -ratio * (ratio - 2);        }        public static function easeInOut (ratio:Number):Number        {            return (ratio < 0.5) ? 2 * ratio * ratio : -2 * ratio * (ratio - 2) - 1;        }    }}