package com.tengu.tween.easing{    public class Cubic    {        public static function easeIn (ratio:Number):Number        {            return ratio * ratio * ratio;        }        public static function easeOut (ratio:Number):Number        {            return (ratio -= 1) * ratio * ratio + 1;        }        public static function easeInOut (ratio:Number):Number        {            return (ratio < 0.5) ? 4 * ratio * ratio * ratio : 4 * (ratio -= 1) * ratio * ratio + 1;        }    }}