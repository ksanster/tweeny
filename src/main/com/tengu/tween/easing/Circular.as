package com.tengu.tween.easing{    public class Circular    {        public static function easeIn (ratio:Number):Number        {            return -(Math.sqrt(1 - ratio * ratio) - 1);        }        public static function easeOut (ratio:Number):Number        {            return Math.sqrt(1 - (ratio - 1) * (ratio - 1));        }        public static function easeInOut (ratio:Number):Number        {            return ((ratio *= 2) < 1) ? -0.5 * (Math.sqrt(1 - ratio * ratio) - 1) : 0.5 * (Math.sqrt(1 - (ratio -= 2) * ratio) + 1);        }    }}