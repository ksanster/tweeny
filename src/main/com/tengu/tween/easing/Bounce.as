package com.tengu.tween.easing{    public class Bounce    {        public static function easeIn (ratio:Number):Number        {            return 1 - easeOut(1 - ratio);        }        public static function easeOut (ratio:Number):Number        {            if (ratio < 1 / 2.75)            {                return 7.5625 * ratio * ratio;            }            else if (ratio < 2 / 2.75)            {                return 7.5625 * (ratio -= 1.5 / 2.75) * ratio + 0.75;            }            else if (ratio < 2.5 / 2.75)            {                return 7.5625 * (ratio -= 2.25 / 2.75) * ratio + 0.9375;            }            else            {                return 7.5625 * (ratio -= 2.625 / 2.75) * ratio + 0.984375;            }        }        public static function easeInOut (ratio:Number):Number        {            return ((ratio *= 2) < 1) ? 0.5 * easeIn(ratio) : 0.5 * easeOut(ratio - 1) + 0.5;        }    }}