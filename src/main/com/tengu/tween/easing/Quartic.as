package com.tengu.tween.easing{    public class Quartic    {        public static function easeIn (ratio:Number):Number        {            return ratio * ratio * ratio * ratio;        }        public static function easeOut (ratio:Number):Number        {            return 1 - (ratio -= 1) * ratio * ratio * ratio;        }        public static function easeInOut (ratio:Number):Number        {            return (ratio < 0.5) ? 8 * ratio * ratio * ratio * ratio : -8 * (ratio -= 1) * ratio * ratio * ratio + 1;        }    }}